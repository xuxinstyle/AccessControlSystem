<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.system.mapper.VisitPageMapper">
	<select id="findByPaging" parameterType="PagingVO" resultType="Visitor"> 
		select * 
		from Visitor
		
		limit #{toPageNo}, #{pageSize}
	</select>
	<select id="findByPagingadmin" parameterType="PagingVO" resultType="Visitor"> 
		select * 
		from Visitor
		
		limit #{toPageNo}, #{pageSize}
	</select>
	<select id="findByPagingVisitor" parameterType="PagingVO" resultType="Visitor"> 
		select * 
		from Visitor
		
		limit #{toPageNo}, #{pageSize}
	</select>
</mapper>